.auth.wrapper.fixed-header.menu-pin
    .login-wrapper
      //- START Login Background Pic Wrapper
      .bg-pic
        //- START Background Pic
        img.lazy(src='assets/img/demo/stefan-stefancik-105376.jpg', data-src='assets/img/demo/stefan-stefancik-105376.jpg', data-src-retina='assets/img/demo/stefan-stefancik-105376.jpg', alt='')
        //- END Background Pic
        //- START Background Caption
        .bg-caption.pull-bottom.sm-pull-bottom.text-white.p-l-20.m-b-20
          //- h2.semi-bold.text-white
            | Turn hesitating visitors
            br
            | into paying customers.
          p.small
            | copyright Â© 2017 HEY.
        //- END Background Caption
      //- END Login Background Pic Wrapper
      //- START Login Right Container
      .login-container.bg-white
        .p-l-50.m-l-20.p-r-50.m-r-20.p-t-50.m-t-30.sm-p-l-15.sm-p-r-15.sm-p-t-40
          img(src='assets/img/logo.png', alt='logo', data-src='assets/img/logo.png', data-src-retina='assets/img/logo_2x.png', width='120')
          p.p-t-35 Sign into your account
          //- START Login Form
          form#form-login.p-t-15(role='form', action='index.html')
            //- START Form Control
            .form-group.form-group-default
              label Login
              .controls
                input.form-control(type='text', name='username', placeholder='User Name', required='')
            //- END Form Control
            //- START Form Control
            .form-group.form-group-default
              label Password
              .controls
                input.form-control(type='password', name='password', placeholder='Credentials', required='')
            //- START Form Control
            .row
              .col-md-6.no-padding.sm-p-l-10
                .checkbox
                  input#checkbox1(type='checkbox', value='1')
                  label(for='checkbox1') Keep Me Signed in
              .col-md-6.d-flex.align-items-center.justify-content-end
                a.text-info.small(href='mailto:info@get-hey.com') Help? Contact Support
            //- END Form Control
            button.btn.btn-primary.btn-cons.m-t-10(type='submit') Sign in
          //- END Login Form
          //- .pull-bottom.sm-pull-bottom
            .m-b-30.p-r-80.sm-m-t-20.sm-p-r-15.sm-p-b-20.clearfix
              .col-sm-3.col-md-2.no-padding
                img.m-t-5(alt='', data-src='assets/img/demo/pages_icon.png', data-src-retina='assets/img/demo/pages_icon_2x.png', height='60', src='assets/img/demo/pages_icon.png', width='60')
              .col-sm-9.no-padding.m-t-10
                p
                  small
                    | Create a pages account. If you have a facebook account, log into it for this
                    | process. Sign in with 
                    a.text-info(href='#') Facebook
                    |  or 
                    a.text-info(href='#') Google
      //- END Login Right Container
